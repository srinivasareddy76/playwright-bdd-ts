      "idleTimeoutMillis": 30000,
      "connectionTimeoutMillis": 2000
    }
  },
  "certs": {
    "client": {
      "pfxPath": "secrets/u1-client.pfx",
      "passphrase": "CHANGE_ME",
      "origin": "https://api.u1.example.com"
    }
